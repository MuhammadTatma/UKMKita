<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbox" prefHeight="497.0" prefWidth="810.0" styleClass="transparent" stylesheets="@../Style/coba.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Halaman15Statistik.ChartController">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="14.0" />
   </padding>
   <children>
      <HBox alignment="TOP_RIGHT" prefHeight="13.0" prefWidth="780.0">
         <children>
            <ChoiceBox fx:id="choiceBox" onMouseClicked="#tampilStatistikPenghasilan" prefHeight="25.0" prefWidth="186.0" visible="false" />
         </children>
         <padding>
            <Insets right="15.0" />
         </padding>
      </HBox>
      <AreaChart fx:id="chart" prefHeight="423.0" prefWidth="800.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" autoRanging="false" side="LEFT" />
        </yAxis>
      </AreaChart>
   </children>
</VBox>
